			half4 frag ( VertexOutput IN  ) : SV_Target
			{
				UNITY_SETUP_INSTANCE_ID( IN );
				UNITY_SETUP_STEREO_EYE_INDEX_POST_VERTEX( IN );

				#if defined(ASE_NEEDS_FRAG_WORLD_POSITION)
				float3 WorldPosition = IN.worldPos;
				#endif
				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 texCoord7 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float U9 = abs( texCoord7.x );
				float4 lerpResult13 = lerp( _ColorBase , _ColorBright , pow( U9 , _Distance ));
				float4 BaseColor17 = lerpResult13;
				float temp_output_17_0_g1 = _PatternTiling;
				float2 appendResult12_g1 = (float2(temp_output_17_0_g1 , temp_output_17_0_g1));
				float2 appendResult2_g1 = (float2(WorldPosition.x , WorldPosition.z));
				float2 panner23 = ( _TimeParameters.x * _PatternSpeed + ( float2( 0,0 ) + ( appendResult12_g1 * appendResult2_g1 ) ));
				float4 PatternBase29 = ( _Pattern * tex2D( _PatternTex, panner23 ) );
				float4 PatternMask46 = ( pow( U9 , _PatternMaskDIstance ) * PatternBase29 );
				float4 blendOpSrc52 = PatternBase29;
				float4 blendOpDest52 = PatternMask46;
				float4 lerpBlendMode52 = lerp(blendOpDest52,( 0.5 - 2.0 * ( blendOpSrc52 - 0.5 ) * ( blendOpDest52 - 0.5 ) ),_PatternMask);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( BaseColor17 + ( ( saturate( lerpBlendMode52 )) * _PatternColor ) ).rgb;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
		}

		
